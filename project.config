[access]
	inheritFrom = Public-Plugins
[submit]
	action = inherit
[project]
	description = Plugin to expose a self-service API and UX to manage accounts and associated personal information.
[access "refs/*"]
	owner = group plugins-account
	exclusiveGroupPermissions = forgeCommitter forgeServerAsCommitter label-Verified push pushMerge
[access "refs/tags/*"]
	createSignedTag = group plugins-account
	createTag = group plugins-account
	create = group plugins-account
	exclusiveGroupPermissions = create createSignedTag createTag viewPrivateChanges
